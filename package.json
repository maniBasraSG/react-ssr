{
  "name": "react-app",
  "version": "1.0.0",
  "description": "react-app",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel dev:*",
    "build": "npm-run-all --parallel deploy:*",
    "dev:server": "nodemon --watch build --exec node build/bundle.js",
    "dev:build-server": "webpack --config webpack.config.server.js --watch",
    "dev:build-client": "webpack  --config webpack.config.client.js  --watch",
    "deploy": "npm run deploy:prod-server && npm run deploy:prod-client",
    "deploy:prod-server": "webpack --config webpack.config.server.js",
    "deploy:prod-client": "webpack --config webpack.config.client.js",
    "format": "prettier --write --ignore-path .gitignore \"**/*.+(js|json|css|html|md)\"",
    "lint": "eslint src/client --fix --color  --ignore-path .gitignore ",
    "pre-commit-lint": "lint-staged",
    "prepare": "husky install",
    "docker": "docker-compose -f docker-compose-local.yml up"
  },
  "author": "Studio Graphene",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-proposal-function-bind": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "extract-css-chunks-webpack-plugin": "^4.9.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.3.0",
    "redux-devtools-extension": "^2.13.8",
    "rimraf": "^3.0.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "7.14.2",
    "@babel/plugin-proposal-logical-assignment-operators": "7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "7.0.0",
    "@babel/plugin-transform-object-assign": "7.0.0",
    "@babel/plugin-transform-react-jsx": "7.14.9",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-react": "7.14.5",
    "@babel/runtime": "^7.17.2",
    "autoprefixer": "^9.7.6",
    "axios": "^0.21.1",
    "babel-loader": "^8.0.0",
    "browser-sync": "^2.26.14",
    "browser-sync-webpack-plugin": "2.2.2",
